{
  "name": "ngx-decorate-preprocessor",
  "version": "1.1.10",
  "bin": {
    "ngx-decorate-preprocess": "cli.js"
  },
  "description": "Preprocess files for ngx-decorate",
  "engines": {
    "node": ">=8.0"
  },
  "main": "index.js",
  "license": "MIT",
  "types": "index.d.ts",
  "typings": "index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "pretest": "rimraf coverage",
    "test": "cross-env RUNNING_TESTS=1 nyc mocha --opts ./mocha.opts",
    "test:watch": "npm run test -- --watch",
    "typecheck": "npm run build -- --noEmit",
    "tslint": "alo tslint -p tsconfig.tslint.json",
    "tslint:fix": "npm run tslint -- --fix",
    "watch": "npm run build -- --watch"
  },
  "repository": "git@github.com:Alorel/ngx-decorate-preprocessor.git",
  "author": "Arturas Molcanovas <a.molcanovas@gmail.com>",
  "private": false,
  "dependencies": {
    "chalk": "^2.4.1",
    "glob": "^7.1.3",
    "lodash": "^4.17.11",
    "tmp": "^0.0.33",
    "ts-simple-ast": "^17.0.1",
    "tslib": "^1.9.3",
    "typescript-lazy-get-decorator": "^1.2.2",
    "yargs": "^12.0.2"
  },
  "devDependencies": {
    "@alorel-personal/build-tools": "4.1.3",
    "@alorel-personal/conventional-changelog-alorel": "2.0.4",
    "@alorel-personal/semantic-release": "1.3.1",
    "@alorel-personal/tslint-rules": "4.0.0",
    "@semantic-release/changelog": "~3.0.0",
    "@semantic-release/exec": "~3.1.2",
    "@semantic-release/git": "~7.0.3",
    "@semantic-release/npm": "~5.0.4",
    "@types/chai": "~4.1.4",
    "@types/cross-spawn": "~6.0.0",
    "@types/fs-extra": "~5.0.4",
    "@types/glob": "~7.1.0",
    "@types/lodash": "~4.14.116",
    "@types/mocha": "~5.2.5",
    "@types/node": "~10.12.0",
    "@types/tmp": "~0.0.33",
    "@types/uuid": "~3.4.4",
    "@types/yargs": "~12.0.0",
    "chai": "~4.2.0",
    "coveralls": "~3.0.2",
    "cross-env": "~5.2.0",
    "cross-spawn": "~6.0.5",
    "fs-extra": "~7.0.0",
    "mocha": "~5.2.0",
    "nyc": "~13.0.1",
    "rimraf": "~2.6.2",
    "semantic-release": "~15.9.12",
    "source-map-support": "~0.5.9",
    "ts-node": "~7.0.1",
    "typescript": "~3.1.0",
    "uuid": "~3.3.2"
  }
