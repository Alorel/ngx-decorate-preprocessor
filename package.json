{
  "name": "ngx-decorate-preprocessor",
  "version": "1.1.18",
  "bin": {
    "ngx-decorate-preprocess": "cli.js"
  },
  "description": "Preprocess files for ngx-decorate",
  "engines": {
    "node": ">=8.0"
  },
  "main": "index.js",
  "license": "MIT",
  "types": "index.d.ts",
  "typings": "index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "pretest": "rimraf coverage",
    "test": "cross-env RUNNING_TESTS=1 nyc mocha --opts ./mocha.opts",
    "test:watch": "npm --scripts-prepend-node-path=auto run test -- --watch",
    "typecheck": "npm --scripts-prepend-node-path=auto run build -- --noEmit",
    "typecheck:watch": "npm --scripts-prepend-node-path=auto run typecheck -- --watch",
    "tslint": "alo tslint -p tsconfig.tslint.json",
    "tslint:fix": "npm --scripts-prepend-node-path=auto run tslint -- --fix",
    "watch": "npm --scripts-prepend-node-path=auto run build -- --watch"
  },
  "repository": "git@github.com:Alorel/ngx-decorate-preprocessor.git",
  "author": "Arturas Molcanovas <a.molcanovas@gmail.com>",
  "private": false,
  "dependencies": {
    "chalk": "^3.0.0",
    "glob": "^7.1.3",
    "lodash": "^4.17.14",
    "tmp": "^0.1.0",
    "ts-morph": "^1.2.0",
    "tslib": "^1.9.3",
    "lazy-get-decorator": "^2.0.0",
    "yargs": "^15.0.1"
  },
  "devDependencies": {
    "@alorel-personal/build-tools": "4.7.19",
    "@alorel-personal/conventional-changelog-alorel": "2.1.1",
    "@alorel-personal/semantic-release": "1.4.0",
    "@alorel-personal/tslint-rules": "5.0.6",
    "@semantic-release/changelog": "~3.0.0",
    "@semantic-release/exec": "~3.3.1",
    "@semantic-release/git": "~7.0.3",
    "@semantic-release/npm": "~5.3.1",
    "@types/chai": "~4.2.0",
    "@types/cross-spawn": "~6.0.0",
    "@types/fs-extra": "~8.0.0",
    "@types/glob": "~7.1.0",
    "@types/lodash": "~4.14.116",
    "@types/mocha": "~5.2.5",
    "@types/node": "~13.1.0",
    "@types/tmp": "~0.1.0",
    "@types/uuid": "~3.4.4",
    "@types/yargs": "~13.0.0",
    "chai": "~4.2.0",
    "coveralls": "~3.0.3",
    "cross-env": "~6.0.0",
    "cross-spawn": "~7.0.0",
    "fs-extra": "~8.1.0",
    "mocha": "~6.2.0",
    "nyc": "~15.0.0",
    "rimraf": "~3.0.0",
    "semantic-release": "~17.0.0",
    "source-map-support": "~0.5.9",
    "ts-node": "~8.5.0",
    "typescript": "~3.6.3",
    "uuid": "~3.3.2"
  }
}
